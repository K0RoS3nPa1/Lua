local allowedIds = loadstring(game:HttpGet("https://raw.githubusercontent.com/K0RoS3nPa1/Lua/refs/heads/main/Lua4"))()

--Main--

M.newInput("Chat bubble", "Prints your text for only bubble.", function(H)
    local Hider = H 

  workspace.Main.Chatted:FireServer (Hider, 'all' )
end)

M.newInput("Chat bubble Spam", "Prints your text.", function(txt)
	_G.Enabled = true 
    local msg = txt 
	   
 
    if _G.Enabled == true then
    while _G.Enabled do
    wait(0.7)
    workspace.Main.Chatted:FireServer (msg, 'all' )
    end
    end	
end)

M.newInput("Chat Spam,", "Prints your text.", function(txt)
	_G.Enabled = true 
    local msg = txt 
	   
 
    if _G.Enabled == true then
    while _G.Enabled do
    wait(0.7)
    workspace.Main.Chatted:FireServer (msg)
    end
    end	
end)

M.newButton("Spam Stop", "", function(txt)
    _G.Enabled = false 
    local msg = txt 
       
 
    if _G.Enabled == true then
    while _G.Enabled do
    wait(0.4)
    game.Workspace.Main.Chatted:FireServer(msg)
    end
    end
end)

M.newToggle("Spam sounds", "", false, function(ab)
    getgenv().hit=ab;while wait()do if getgenv().hit then for j,k in pairs(game.Workspace:GetDescendants())do if k:IsA("Sound")then k:Play()end end end end
end)

local replicatedStorage = game:GetService("ReplicatedStorage") -- Sicherer Speicher f√ºr die Modelle
local model1 = game.Workspace:FindFirstChild("KrustyKrab") -- Modell 1 finden
local model2 = game.Workspace:FindFirstChild("TeleportToPartyIsland")  -- Modell 2 finden

-- Modelle in ReplicatedStorage sichern, falls sie nicht schon dort sind
if model1 then model1.Parent = replicatedStorage end
if model2 then model2.Parent = replicatedStorage end

M.newToggle("Anti Teleports", "", false, function(state)
    if state then
        -- Wenn der Toggle aktiviert wird, Modelle ausblenden (in ReplicatedStorage verschieben)
        if model1 then model1.Parent = replicatedStorage end
        if model2 then model2.Parent = replicatedStorage end
    else
        -- Wenn der Toggle deaktiviert wird, Modelle wieder ins Workspace verschieben
        if model1 then model1.Parent = game.Workspace end
        if model2 then model2.Parent = game.Workspace end
    end
end)

M.newToggle("Anti-Oven Fire", "", false, function(y)
    getgenv().heee=y;while true do wait(.40)if getgenv().heee then pcall(function()for j,k in pairs(game.Players:GetPlayers())do local g={[1]=k.Character.UpperTorso}workspace.GameService.ExtinguishFire:FireServer(unpack(g))for j,k in pairs(game.Workspace.AllDough:GetChildren())do local g={[1]=k}workspace.GameService.ExtinguishFire:FireServer(unpack(g))end end end)end end
end)

M.newToggle("Anti sit", "", false, function(sit)
  	    local RunService = game:GetService("RunService")
			function antiSit()
			if game.Players.LocalPlayer.Character.Humanoid:GetState() == Enum.HumanoidStateType.Seated then 
			game.Players.LocalPlayer.Character.Humanoid.Jump = sit
			end
			end
			RunService:BindToRenderStep("tempBinding", 1000, antiSit)
end)





