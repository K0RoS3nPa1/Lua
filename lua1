--[Owner]

local success, allowedIds = pcall(function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/K0RoS3nPa1/Lua/refs/heads/main/Lua4"))()
end)

if success and type(allowedIds) == "table" and table.find(allowedIds, PlayerId) then
    local O = DrRayLibrary.newTab("Owner", "rbxassetid://2795572800")

    O.newButton("become manager", "", function()
        spawn(loadstring(game:HttpGet("https://raw.githubusercontent.com/incognitototo/sda/main/dwa.lua",true)))
    end)

    O.newButton("Pizza place", "kick, Tolling version", function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/Hm5011/hussain/refs/heads/main/Work%20at%20a%20pizza%20place'),true))()
    end)

    O.newButton("Remotespy", "", function()
        local owner = "Upbolt"
        local branch = "revision"
        
        local function webImport(file)
            return loadstring(game:HttpGetAsync(("https://raw.githubusercontent.com/%s/Hydroxide/%s/%s.lua"):format(owner, branch, file)), file .. '.lua')()
        end
        
        webImport("init")
        webImport("ui/main")
    end)

    O.newButton("local Position", "", function()
        local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
        setclipboard(tostring(pl.CFrame))
    end)


    O.newInput("Pose Test", "Print CFrame", function(CFrame)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(CFrame)
    end)
else
    print("Kein Zugriff!")
end
