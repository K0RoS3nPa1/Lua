M.newToggle("Anti sit", "", false, function(sit)
  	    local RunService = game:GetService("RunService")
			function antiSit()
			if game.Players.LocalPlayer.Character.Humanoid:GetState() == Enum.HumanoidStateType.Seated then 
			game.Players.LocalPlayer.Character.Humanoid.Jump = sit
			end
			end
			RunService:BindToRenderStep("tempBinding", 1000, antiSit)
end)
M.newButton("Antibann", "", function(bn)
    local bn = getrawmetatable(game)
    make_writeable(bn)
    
    local namecall = bn.__namecall
    
    bn.__namecall = newcclosure(function(self, ...)
        local method = getnamecallmethod()
        local args = {...}
    
        if method == "FireServer" and tostring(self) == "PlayerChannel" then
           return
        end
        return namecall(self, table.unpack(args))
    end)
    end)

